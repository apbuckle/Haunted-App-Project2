<h1>All Venues <a href='/venues/new'>New</a></h1> 

<div>
    {{#each Venues}}
        <div>name: {{this.name}} <a href='/venues/{{this._id}}'>Go to Venue</a>  </div>
        <div>city: {{this.city}} </div>
        <div>age: {{this.age}}</div>
        <form action='/venues/{{this._id}}?_method=DELETE' method='POST'>
    <input type="submit" value="Delete Venue">
</form>

        <br>
    {{/each}}

</div>